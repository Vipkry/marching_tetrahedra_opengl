package com.system;

public class CPU{
	private Process executing;
	private int coreId;
	int quantumCounter;
	
	public CPU (int coreId) {
		this.coreId = coreId;
	}

	public void execute()  {
		
	}
	
	public boolean empty (){
		return executing == null;
	}
	
	public Process getExecuting (){
		return executing;
	}

	public String toString (){
		return "CPU" + this.coreId;
	}
	
}
